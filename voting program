#include<iostream>
using namespace std;
int main()
{
	string name;
	int age;
	long int voter_id_no;
	cout<<"Enter your name : ";
	cin>>name;
	cout<<"Enter your age : ";
	cin>>age;
	if(age<18)
	{
		cout<<"Sorry!"<<endl<<"You are not eligible for voting ";
	}
	else
	{
		cout<<"You are eligible for voting "<<endl;
		cout<<"Enter your voter id number :";
		cin>>voter_id_no;
		cout<<"\nNow you can vote "<<name;
		
	}
}
